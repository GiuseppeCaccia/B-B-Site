    <div id="content-wrapper" class="d-flex flex-column">
        <%- include('../../script/NavbarProp.ejs') %>

				<form class="f-ins" action='/invioModifica' method='POST'>
					<div>
						<div>
							<div class="section over-hide z-bigger">
								<label class="for-checkbox" for="general"></label>
								<div class="section over-hide z-bigger">
									<div class="container pb-5">
										<div class="row justify-content-center pb-5">
											<div class="wrap-input100 rs1-wrap-input100 validate-input" data-validate="Il nome della struttura è richiesto e non può superare i 20 caratteri">
												<span class="label-input100">Nome Struttura</span>
												<input class="input100" type="text" name="nome_str_nuova" maxlength="20" placeholder="Inserisci il nome della tua struttura" value="<%=struttura.nome_str%>" required>
												<span class="focus-input100"></span>
											</div>
											<div class="wrap-input100 rs1-wrap-input100 " >
												<table class="camere-ins">
													<tr>
														<th class="th"> Indirizzo </th>
														<th class="th"> civico </th>
														<th class="th"> comune </th>
														<th class="th"> cap </th>
													</tr>
													<tr>
														<td>  <input class="input100" type="text" name="indirizzo" placeholder="via..." value="<%=struttura.indirizzo%>" required>
															<span class="focus-input100"></span>
														</td>
														<td>  <input class="input100" type="number" name="civico"  placeholder="" value="<%=struttura.civico%>" required>
															<span class="focus-input100"></span> </td>
														<td>  <input class="input100" type="text" name="comune"  placeholder="" value="<%=struttura.comune%>" required>
															<span class="focus-input100"></span> </td>
															<td>  <input class="input100" type="number" name="cap"  placeholder="" value="<%=struttura.cap%>" required>
																<span class="focus-input100"></span> </td>
													</tr>
												</table>
											</div>



											<div class="wrap-input100 rs1-wrap-input100 " >
												<table class="camere-ins">
													<tr>
                              <% if (struttura.tipologia == "Casa Vacanza") { %>
  														  <th class="th"> Prezzo a notte</th>
  														<% }
  														else { %>
  															<th class="th"> Prezzo a persona</th>
  														<% } %>
														<th class="th"> Distanza dal centro</th>
														<th class="th"> Tassa di Soggiorno</th>
														<th class="th"> Telefono</th>
													</tr>
													<tr>
														<td><input class="input100" type="int" name="prezzo" min='1' placeholder="€ 0" value="<%=struttura.prezzo%>" required>
															<span class="focus-input100"></span>  </td>
														<td> <input class="input100" type="int" name="distanza" min='1' placeholder="Km 0" value="<%=struttura.distanza_km_centro%>" required>
															<span class="focus-input100"></span>  </td>
														<td> <% if (struttura.tipologia == "Casa Vacanza") { %>
															<input class="input100" type="int" name="tassa" min='0' placeholder="€ 0" value="0" disabled>
														<% }
														else { %>
															<input class="input100" type="int" name="tassa" min='0' placeholder="€ 0" value="<%=struttura.tassa_di_soggiorno%>" required>
														<% } %>
															<span class="focus-input100"></span>  </td>
														<td> <input class="input100" type="number" name="telefono" min='1' placeholder="" value="<%=struttura.telefono%>" required>
															<span class="focus-input100"></span>  </td>
													</tr>
													<tr>
														<td> <i class="fas fa-coins"></i> </td>
														<td> <i class="fas fa-car"></i></td>
														<td><i class="fas fa-hotel"></i></td>
														<td><i class="fas fa-phone"></i></td>
													</tr>
												</table>
											</div>
											<div class="wrap-input100 validate-input" data-validate = "La descrizione della struttura è richiesta">
												<span class="label-input100">Descrizione Struttura</span>
												<textarea class="input100" name="descrizione" placeholder="Scrivi una breve descrizione della tua struttura" required><%=struttura.descrizione_struttura%></textarea>
												<span class="focus-input100"></span>
											</div>


											<div class="wrap-input100 " >
												<table class="camere-ins">
													<tr>
														<th class="th">Camera singola </th>
														<th class="th">Camera doppia </th>
														<th class="th">Camera tripla </th>
														<th class="th">Camera quadrupla </th>
													</tr>
													<tr>
														<td>
															<input class="input100" type="number" name="singola" min='0' value="<%=struttura.camera_singola%>" placeholder="0" required>
															<span class="focus-input100"></span>
														</td>
														<td>
															<input class="input100" type="number" name="doppia" min='0' placeholder="0" value="<%=struttura.camera_doppia%>" required>
															<span class="focus-input100"></span>
														</td>
														<td>
															<input class="input100" type="number" name="tripla" min='0' placeholder="0" value="<%=struttura.camera_tripla%>" required>
															<span class="focus-input100"></span>
														</td>
														<td>
															<input class="input100" type="number" name="quadrupla" min='0' placeholder="0" value="<%=struttura.camera_quadrupla%>" required>
															<span class="focus-input100"></span>
														</td>
													</tr>
													<tr>
														<td>
															<i class="fas fa-bed"></i>
														</td>
														<td>
															<i class="fas fa-bed"></i>
															<i class="fas fa-bed"></i>
														</td>
														<td>
															<i class="fas fa-bed"></i>
															<i class="fas fa-bed"></i>
															<i class="fas fa-bed"></i>
														</td>
														<td>
															<i class="fas fa-bed"></i>
															<i class="fas fa-bed"></i>
															<i class="fas fa-bed"></i>
															<i class="fas fa-bed"></i>
														</td>
													</tr>
												</table>
											</div>

											<div class="wrap-input100 " >
												<div class="col-12 pb-5">
													<%- include('../../script/Servizi.ejs') %>
												</div>
											</div>
											<input type="hidden" name="nome_str_vecchia" value="<%=struttura.nome_str%>"/>
											<div class="container-contact100-form-btn">
												<button class="contact100-form-btn">
													<span>
														Conferma <i class='fas fa-caret-right'></i>
													</span>
												</button>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</form>
            	</div>

